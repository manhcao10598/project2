<body>
  <div class="table-responsive text-nowrap">

    <table class="table">
      <thead>
        <tr>
          <th scope="col">Book</th>
          <th scope="col">Detail</th>
        </tr>
      </thead>
      <tbody>
        <% for(var j = 0 ; j < data[0].bookId.length ; j++){ %>
        <tr>
          <td> <%= data[0].bookId[j].name %> </td>

          <td> <button class="btn btn-update btn-primary">Update</button> <button
              class="btn btn-delete btn-danger">Delete</button> </td>
        </tr>
        <% } %>

      </tbody>
    </table>

  </div>
</body>
